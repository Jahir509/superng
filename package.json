{
  "name": "super-ng",
  "version": "0.0.0",
  "scripts": {
    "lint": "eslint . --ext .ts,.js && stylelint '**/*.scss'",
    "format": "prettier --write .",
    "pre-commit": "pretty-quick --staged",
    "i18n:extract": "ngx-translate-extract --input ./src --output ./src/assets/i18n/en.json",
    "ng": "ng",
